# ...to here.
# Sort the stop times by field 1 (need to figure out what that is).
sort stop_times.txt >> stop_times_sorted.txt
more stop_times_sorted.txt 
join -1 3 -2 1 trips_8.txt stop_times_sorted.txt | more
cat trips_8.txt 
# had to add the delimiter (comma).
join -t, -1 3 -2 1 trips_8.txt stop_times_sorted.txt
join -t, -1 3 -2 1 trips_8.txt stop_times_sorted.txt >> stop_times_8.txt
more trips_8.txt 
man join
man sort
ll
head trips_8.txt 
sort trips_8.txt --key=3 | more
# sort on the 3rd field of the route-8 trips file
sort trips_8.txt --key=3 >> trips_8_sorted.txt
# this was the join that I think finally worked to extract the 8 stops
join -t, -1 3 -2 1 trips_8_sorted.txt stop_times_sorted.txt > stop_times_8.txt
more trips_8_sorted.txt 
man sort
# or maybe i had to add the delimiter to the sort first then i ran
# the same join above.  
# Somewhere in here my terminal crashed so I think i lost some history.
sort -t, trips_8.txt --key=3 > trips_8_sorted.txt
more trips_8_sorted.txt 
