more stop_id_sorted.txt 
# Here's another join that might have been the one that ultimately worked.
# Will have to explore and try to reproduce the results.
echo "join -t, -1 3 -2 1 trips_8_sorted.txt stop_times_sorted.txt > stop_times_8.txt"
head stops.txt 
joint -t, -1 1 -2 10 stops.txt stop_id_sorted.txt >> stops8.txt
# Then here is an attempt to link up the stops (with stop-specific data
# like lat,lon stuff) with the route-8 stops.
join -t, -1 1 -2 10 stops.txt stop_id_sorted.txt >> stops8.txt
sort -t, stops.txt  | more
sort -t, stops.txt >> stops_all_sorted.txt
join -t, -1 1 -2 10 stops_all_sorted.txt stop_id_sorted.txt > stops8.txt
more stops8.txt 
head -n1 stops8.txt
echo "trip_id,route_id,service_id,trip_headsign,block_id,shape_id,wheelchair_accessible,arrival_time,departure_time,stop_id,stop_sequence,pickup_type,drop_off_type"
